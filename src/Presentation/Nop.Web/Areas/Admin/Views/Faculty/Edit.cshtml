@model Nop.Web.Areas.Admin.Models.Corporations.FacultyModel

@{
    //page title
    ViewBag.PageTitle = T("Admin.Corporations.Faculties.EditDetails").Text;
   
    //active menu item (system name)
    NopHtml.SetActiveMenuItemSystemName("Faculties");
}
<form asp-controller="Faculty" asp-action="Edit" method="post">
    <div class="content-header clearfix">
        <h1 class="float-left">
            @T("Admin.Corporations.Faculties.EditDetails") - @Model.Name
            <small>
                <i class="fas fa-arrow-circle-left"></i>
                <a asp-action="List">@T("Admin.Common.BackToList")</a>
            </small>
        </h1>
        <div class="float-right">
            <span id="faculty-delete" class="btn btn-danger">
                <i class="far fa-trash-alt"></i>
                @T("Admin.Common.Delete")
            </span>
        </div>
    </div>
    @await Html.PartialAsync("_CreateOrUpdate", Model)
</form>
<nop-delete-confirmation asp-model-id="@Model.Id" asp-button-id="faculty-delete"/>